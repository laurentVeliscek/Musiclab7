script/source = "extends Control

var TAG = \"Pattern\"

# index du StrumPattern courant
var current:int = 0 

#onready var guitar_player:FolkGuitarPlayer = null
onready var step_beat_length_btn:Button = $step_beat_length_btn
onready var pattern_number_sb:SpinBox = $pattern_number_sb
onready var guitar_player_scene = $\"..\"
onready var delete_btn = $delete_btn
onready var info_pattern_label:Label = $info_pattern_label
onready var pattern = $\".\"
# Called when the node enters the scene tree for the first time.
func _ready():
	delete_btn.hide()
	#guitar_player = $\"..\".guitar_player
	#current_strum_pattern = guitar_player.pattern_sequence[0]


#func _update_pattern(pat:String,step_length:float):
#	var currentStrumPattern:StrumPattern = guitar_player_scene.guitar_player.pattern_sequence[current]
#	currentStrumPattern.pattern = pat
#	currentStrumPattern.step_beat_length = step_length

func _update_pattern_display():
	yield(get_tree(), \"idle_frame\") 
	
	var currentStrumPattern:StrumPattern = guitar_player_scene.guitar_player.pattern_sequence[current]
	pattern_number_sb.value = current + 1
	#$step_beat_length_btn.set_step_length(currentStrumPattern.step_beat_length)
	step_beat_length_btn.step_length = currentStrumPattern.step_beat_length
	$pattern_lineEdit.text = currentStrumPattern.pattern.rstrip(\" \")
	guitar_player_scene.set_player_config(currentStrumPattern)
	var info_txt = \"Pattern \" + str(current+1) + \" / \" + str(guitar_player_scene.guitar_player.pattern_sequence.size())
	info_txt += \"  - duration: \" + str(guitar_player_scene.guitar_player._calculate_patterns_duration()) + \" measures\"
	info_pattern_label.text = info_txt

func _on_delete_btn_pressed():
	var seq = guitar_player_scene.guitar_player.pattern_sequence
	if seq.size() > 1:
		seq.remove(current)
		$pattern_number_sb._on_pattern_number_sb_value_changed(current) 
		if seq.size() == 1:
			delete_btn.hide()
		
	
	_update_pattern_display()
	
	$\"..\".clear_console()
	$\"..\".midi_player.stop()
	LogBus.info(TAG,\"Pattern deleted\")
	



func _on_generate_btn_pressed():
	guitar_player_scene.rewind()
	var generator = GuitarPatternGenerator.new()
	var style = \"\"
	match $pattern_style_ob.selected :
		0: style = \"Folk\"
		1: style = \"Funky\"
		2: style = \"R'n B\"
		3: style = \"Bossa\"
	#LogBus.debug(TAG, style)
	var answer = generator.generate(style)
	#return {\"pattern\": pattern, \"step_beat_length\": step}
	#$pattern_lineEdit.text = answer[\"pattern\"]
	#$step_beat_length_btn.set_step_length(answer[\"step_beat_length\"])
	var currentStrumPattern:StrumPattern = guitar_player_scene.guitar_player.pattern_sequence[current]
	currentStrumPattern.pattern = answer[\"pattern\"]
	var step_beat = max(answer[\"step_beat_length\"] *2 ,.25)
	
	currentStrumPattern.step_beat_length = step_beat
	_update_pattern_display()
	guitar_player_scene.clear_console()
	LogBus.info(TAG,style +\" Pattern generated\")
	LogBus.debug(TAG,\"beat-> \" + str(answer[\"step_beat_length\"]))
	#guitar_player_scene._on_playStop_btn_pressed()

func _on_pattern_style_ob_item_selected(index):
	yield(get_tree(), \"idle_frame\") 
	_on_generate_btn_pressed()
	
"

