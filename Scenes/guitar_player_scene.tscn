script/source = "extends LineEdit

const TAG =\"lineEdit\"
var ALLOWED = \" .DdUu01234xXBbFfwW\"
onready var guitar_player_scene =  $\"../..\"
onready var pattern = $\"..\"
# drapeau pour éviter la boucle quand on modifie .text dans text_changed
var _ignore = false

func _ready():
	connect(\"text_changed\", self, \"_on_text_changed\")


func _on_text_changed(new_text):
	guitar_player_scene.rewind()
	if _ignore:
		return
	
	# mémoriser caret
	var old_caret = caret_position

	# filtrer texte
	var filtered = \"\"
	var removed_before_caret = 0

	var i = 0
	while i < new_text.length():
		var c = new_text[i]
		if c in ALLOWED:
			filtered += c
		else:
			# si le caractère supprimé était avant le caret, on décale
			if i < old_caret:
				removed_before_caret += 1
		i += 1

	var currentStrumPattern:StrumPattern = guitar_player_scene.guitar_player.pattern_sequence[pattern.current]
	currentStrumPattern.pattern = filtered.rstrip(\" \")
	#LogBus.debug(TAG,\"\\n>\"+  currentStrumPattern.pattern + \"<\")
	

	# si rien à corriger, ne touche pas au caret
	if filtered == new_text:
		return
	
	# appliquer texte filtré
	_ignore = true
	text = filtered
	_ignore = false
	

	
	# ajuster caret
	var new_caret = old_caret - removed_before_caret
	if new_caret < 0:
		new_caret = 0
	if new_caret > filtered.length():
		new_caret = filtered.length()

	caret_position = new_caret



func _gui_input(event):
	
	
	
	if event is InputEventKey and event.pressed:
		# --- Détection de Enter / Return ---
		if event.scancode == KEY_ENTER :
			# Ici tu mets ce que tu veux faire quand Enter est pressé
			print(\"ENTER / RETURN détecté :\", text)
			LogBus.debug(TAG,\"ENTER DETECTED -> PREVIEW !\")
			# Tu peux remplacer par emit_signal(), callback parent, etc.
			accept_event()
			return
		
		
		var _char = OS.get_scancode_string(event.scancode)
		#_char = _char.to_lower()

		if not (_char in ALLOWED):
			accept_event()
"

