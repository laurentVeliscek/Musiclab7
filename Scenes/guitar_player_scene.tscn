script/source = "extends SpinBox

onready var guitar_player_scene = $\"../..\"
onready var pattern = $\"..\"
onready var previous_btn = $\"../previous_btn\"
onready var next_btn = $\"../next_btn\"

# Called when the node enters the scene tree for the first time.
func _ready():
	$\".\".value = 1
	next_btn.hide()
	previous_btn.hide()


# Called every frame. 'delta' is the elapsed time since the previous frame.
#func _process(delta):
#	pass


func _on_pattern_number_sb_value_changed(value):
	var pattern_seq = guitar_player_scene.guitar_player.pattern_sequence
	value = clamp(value,1,pattern_seq.size())
	pattern.current = value -1
	if pattern.current  < 1:
		previous_btn.hide()
	else :
		previous_btn.show()
	
	if pattern.current < pattern_seq.size() -1:
		next_btn.show()
	else:
		next_btn.hide()
	
		
		
	pattern._update_pattern_display()

	#$\".\".value = value
	


func _on_previous_btn_pressed():
	self.value = self.value -1
	pattern._update_pattern_display()
	


func _on_next_btn_pressed():
	self.value = self.value + 1
	pattern._update_pattern_display()
"

