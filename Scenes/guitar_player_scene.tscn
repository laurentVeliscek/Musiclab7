script/source = "extends HBoxContainer

var TAG = \"ConfigSliders\"		#à adapter selon ton LogBus
onready var guitar_player_scene = $\"../../..\"
onready var pattern = $\"../../../Pattern\"

func _ready():
	
	_connect_all_sliders()
	
	var SP:StrumPattern = StrumPattern.new()
	guitar_player_scene.set_player_config(SP)


func _connect_all_sliders():
	print(\"get_children().size(): \" + str(get_children().size()))
	for child in get_children():
		if child is VSlider:
			print(\"connect \"+ str(child))
			# connecter chaque slider à la même méthode
			child.connect(\"value_changed\", self, \"_on_slider_value_changed\", [child])
			#LogBus.debug(TAG, \"connecté : \" + child.get_name())	#pour debug si besoin


func _on_slider_value_changed(value, slider):
	
	guitar_player_scene.clear_console()
	#guitar_player_scene.midi_player.stop()
	guitar_player_scene.rewind()
	
	var slider_name = slider.get_name().replace(\"_\",\" \")
	LogBus.info(TAG, capitalize_words(slider_name) + \"\\n-> \" + str(value))
	var index_pattern = pattern.current 
	var sp:StrumPattern = guitar_player_scene.guitar_player.pattern_sequence[index_pattern]
	sp.config_override = guitar_player_scene.get_player_config()

func capitalize_words(text):
	var parts = text.split(\" \")
	var result = \"\"
	var i = 0

	for p in parts:
		if p.length() > 0:
			var first = p[0].to_upper()
			var rest = \"\"
			if p.length() > 1:
				rest = p.substr(1, p.length() - 1)
			p = first + rest

		if result == \"\":
			result = p
		else:
			result += \" \" + p

	return result
"

