script/source = "extends Button


# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"
const TAG = \"step_length_btn\"
onready var button = $\".\"
onready var pattern = $\"..\"
onready var guitar_player_scene = $\"../..\"

var step_length:float = .5 setget set_step_length,get_step_length

# Called when the node enters the scene tree for the first time.
func _ready():
	pass # Replace with function body.
	
func set_step_length(l:float):
	step_length = l
	match l:
		.25: button.text = \"$\"
		.5: button.text = \".\"
		1.0: button.text = \")\"
	#var gp:FolkGuitarPlayer = pattern.guitar_player
	#var sp:StrumPattern = guitar_player_scene.guitar_player_scene.guitar_player.pattern_sequence[pattern.current]
	#sp.step_beat_length = step_length
# Called every frame. 'delta' is the elapsed time since the previous frame.
#func _process(delta):
#	pass
	
func get_step_length()->float:
	
	match button.text:
		\"$\":return .25
		\".\":return .5
		\")\":return 1.0
		_:return .5

func _on_step_beat_length_btn_pressed():
	guitar_player_scene.rewind()
	var l:float
	match button.text :
		\"$\":
			button.text = \".\"
			l = .5
		\".\":
			button.text = \")\"
			l = 1.0
		\")\":
			button.text = \"$\"
			l = 0.25
	#var gp:FolkGuitarPlayer = pattern.guitar_player
	var sp:StrumPattern = guitar_player_scene.guitar_player.pattern_sequence[pattern.current]
	sp.step_beat_length = l
	guitar_player_scene.clear_console()
	pattern._update_pattern_display()
	LogBus.info(TAG,\"Pattern length set to \"+str(16 * l)+ \" beats\")

func update_step_beat_display(value):
	step_length = value
